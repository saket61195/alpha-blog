<h1>create Article</h1>

<% if @article.errors.any? %>
<ul>
<% @article.errors.full_messages.each do |msg| %>

<li>   <%= msg %>  </li>
<% end %>
</ul>
<% end %>




<%= form_for(:article ,url: new_article_path, method: :create)do |f| %>


<p>

<%= f.label :title %><br/>

<%= f.text_field :title %>

</p>

<p>

<%= f.label :description %><br/>

<%= f.text_area :description %>

</p>

<p>

<%= f.submit :create %>

</p>



<% end %>
